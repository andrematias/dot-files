#! /bin/zsh
alias vim="sudo nvim"
alias sudo="sudo -sE"
alias cat="bat"
alias em="sudo -sE emerge"
alias emerge="sudo -sE emerge"
alias remote="xfreerdp /timeout:36000 /d:lmcorp /dynamic-resolution /log-level:FATAL /cert:ignore +home-drive +clipboard"
alias say="cowsay -r"
alias open="xdg-open"
alias fontlist="fc-list -f '%{family}\n'"
alias notes="zet"
alias f="fzf"
alias files="ranger"
alias ff="ranger"
